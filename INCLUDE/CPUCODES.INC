; CPU id codes

CPU_8088	EQU	1	; 8088
CPU_8086	EQU	2	; 8086
CPU_V20 	EQU	3	; NEC V20
CPU_V30 	EQU	4	; NEC V30
CPU_80188	EQU	5	; 80188
CPU_80186	EQU	6	; 80186
CPU_80286	EQU	7	; 80286
CPU_80386	EQU	8	; 80386
CPU_80386SX	EQU	9	; 80386 SX
CPU_80486	EQU    10	; 80486
CPU_80486SX	EQU    11	; 80486 SX
CPU_PENTIUM	EQU    12	; Pentium CPU
CPU_PENTPRO	EQU    13	; Pentium Pro CPU

; FPU id codes

FPU_NONE	EQU	0	; None installed
FPU_8087	EQU	1	; 8087
FPU_80287	EQU	2	; 80287
FPU_80387	EQU	3	; 80387
FPU_EMULATED	EQU	4	; Emulated
FPU_BUILTIN	EQU	5	; Built-in (i.e. 486)
FPU_80387SX	EQU	6	; 80387 SX

